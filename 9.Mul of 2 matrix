#include <stdio.h>

void multiplyMatrices(int firstMatrix[][2], int secondMatrix[][2], int result[][2], int rowFirst, int columnFirst, int rowSecond, int columnSecond) {
    for (int i = 0; i < rowFirst; ++i) {
        for (int j = 0; j < columnSecond; ++j) {
            result[i][j] = 0;
            for (int k = 0; k < columnFirst; ++k) {
                result[i][j] += firstMatrix[i][k] * secondMatrix[k][j];
            }
        }
    }
}

void display(int result[][2], int row, int column) {
    printf("\nOutput Matrix:\n");
    for (int i = 0; i < row; ++i) {
        for (int j = 0; j < column; ++j) {
            printf("%d  ", result[i][j]);
            if (j == column - 1)
                printf("\n");
        }
    }
}
int main() {
    int firstMatrix[2][2] = {{1, 2}, {3, 4}};
    int secondMatrix[2][2] = {{2, 0}, {1, 2}};
    int result[2][2];
    int rowFirst = 2, columnFirst = 2, rowSecond = 2, columnSecond = 2;

    multiplyMatrices(firstMatrix, secondMatrix, result, rowFirst, columnFirst, rowSecond, columnSecond);

    display(result, rowFirst, columnSecond);


    return 0;
}
